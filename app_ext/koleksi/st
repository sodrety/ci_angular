CREATE TABLE IF NOT EXISTS `kt` (
  `nrk` varchar(222) DEFAULT NULL,
  `inang` text,
  `lokasi` text,
  `gps_ns_a` int(11) DEFAULT NULL,
  `gps_ns_b` int(11) DEFAULT NULL,
  `gps_ns_c` int(11) DEFAULT NULL,
  `gps_ns` varchar(222) DEFAULT NULL,
  `gps_we_a` int(11) DEFAULT NULL,
  `gps_we_b` int(11) DEFAULT NULL,
  `gps_we_c` int(11) DEFAULT NULL,
  `gps_we` varchar(222) DEFAULT NULL,
  `sumber` text,
  `tgl_koleksi` datetime DEFAULT NULL,
  `kolektor` varchar(222) DEFAULT NULL,
  `jenis_spes` varchar(222) DEFAULT NULL,
  `jenis_fam` varchar(222) DEFAULT NULL,
  `jenis_ordo` varchar(222) DEFAULT NULL,
  `stadia` varchar(222) DEFAULT NULL,
  `mounting` varchar(222) DEFAULT NULL,
  `jumlah_spesimen` varchar(222) DEFAULT NULL,
  `tgl_ident` datetime DEFAULT NULL,
  `ident_oleh` varchar(222) DEFAULT NULL,
  `foto` varchar(222) DEFAULT NULL,
  `lokasi_simpan` varchar(222) DEFAULT NULL,
  `ket` varchar(222) DEFAULT NULL,
  `entri_oleh` varchar(222) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
